# Copy this file to .env.local and fill in your values
# Note: Variables must start with VITE_ to be exposed to Vite client code

# n8n webhook URL for form extraction
VITE_WEBHOOK_URL=http://localhost:5678/webhook/tax-refund

# Optional: CORS proxy URL (if CORS not enabled on n8n backend)
# Example: https://cors-anywhere.herokuapp.com/
# Leave empty to disable proxy
VITE_CORS_PROXY=
